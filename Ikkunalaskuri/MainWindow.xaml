        <TextBox x:Name="textBox_Copy" HorizontalAlignment="Left" Height="23" Margin="268,66,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox_Copy1" HorizontalAlignment="Left" Height="23" Margin="268,102,0,0" TextWrapping="Wrap" Text="45" VerticalAlignment="Top" Width="120"/>

